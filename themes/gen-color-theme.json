{
  "tokenColors": [
    {
      "scope": "gen.script.keyword.declaration",
      "settings": {
        "foreground": "#86B1C1"
      }
    },
    {
      "scope": "gen.script.keyword.function",
      "settings": {
        "foreground": "#73bad4"
      }
    },
    {
      "scope": "storage.type.rust",
      "settings": {
        "foreground": "#ed8768"
      }
    },
    {
      "scope": "keyword.control.rust",
      "settings": {
        "foreground": "#799ad4"
      }
    },
    {
      "scope": ["support.function.macro.rust", "meta.attribute.rust"],
      "settings": {
        "foreground": "#ffe4a4" // Green for <style> tags
      }
    },
    {
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "entity.name.tag.gen"
      ],
      "settings": {
        "foreground": "#109aa9" // Green for <style> tags
      }
    },
    {
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#77B0AA" // Purple for IDs
      }
    },
    {
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#7cbeb7" // Red for classes
      }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#c7fff0"
      }
    },
    {
      "scope": [
        "support.constant.property-value.css",
        "string.quoted.double.rust",
        "variable.other.rust",
        "constant.numeric.rust"
      ],
      "settings": {
        "foreground": "#e5fff8"
      }
    }
  ]
}
