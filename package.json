{
  "name": "gen-lsp",
  "displayName": "gen-lsp",
  "description": "lsp for gen-ui",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    
    "languages": [
      {
        "id": "gen",
        "aliases": [
          "Gen",
          "gen"
        ],
        "extensions": [
          ".gen"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gen",
        "scopeName": "source.gen",
        "path": "./syntaxes/gen.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/vscode": "^1.88.0",
    "typescript": "^5.4.5",
    "vscode-test": "^1.6.1"
  }
}
