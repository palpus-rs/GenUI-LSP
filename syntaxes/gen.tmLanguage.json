{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Gen",
  "fileTypes": ["gen"],
  "patterns": [
      {
        "name": "tag.name.gen",
        "begin": "<",
        "end": ">",
        "patterns": [
          {
            "match": "([a-zA-Z0-9]+)",
            "name": "entity.name.tag.gen"
          }
        ] 
      }
  ],
  
  "scopeName": "source.gen"
}
